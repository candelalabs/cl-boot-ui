{
  "name": "cl-boot-ui",
  "version": "1.0.0",
  "keywords": [
    "web",
    "react",
    "typescript",
    "bootstrap"
  ],
  "main": "./dist/main.js",
  "typings": "./@types/src/main",
  "author": "arifnpm",
  "files": [
    "dist",
    "@types"
  ],
  "scripts": {
    "build:ts": "webpack",
    "build:ts:prod": "cross-env NODE_ENV=production webpack",
    "build:sass:prod": "node-sass --output-style compressed ./src/sass/main.scss > ./dist/style.min.css",
    "storybook": "start-storybook -s ./demos/public -p 6006",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:github": "storybook-to-ghpages",
    "pretty": "prettier --use-tabs --write \"{src,demos,tests}/**/*.{ts,tsx}\" --print-width 120",
    "lint": "tslint -p .",
    "test:ci": "rm -rf ./dist && node ./tests/e2e/generate-e2e.js && jest --no-cache",
    "test": "jest --no-cache",
    "prepublishOnly": "yarn run build:ts:prod && yarn run build:sass:prod",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@storybook/cli": "^5.0.11",
    "classnames": "^2.2.6",
    "closest": "^0.0.1",
    "clsx": "^1.0.4",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-transition-group": "^4.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.0-alpha.36",
    "@storybook/addon-options": "^5.1.0-alpha.36",
    "@storybook/addon-storyshots": "^5.1.0-alpha.36",
    "@storybook/addons": "^5.1.0-alpha.36",
    "@storybook/react": "^5.1.0-alpha.36",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.0.4",
    "@types/promise": "^7.1.30",
    "@types/react": "^16.3.13",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.4",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "json-beautify": "^1.0.1",
    "node-sass": "^4.9.0",
    "prettier": "^1.12.1",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react-dom": "^16.3.2",
    "react-syntax-highlighter": "^10.2.1",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "storybook-host": "^5.0.3",
    "storybook-readme": "^3.3.0",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.4",
    "ts-loader": "^5.2.1",
    "tslint": "^5.10.0",
    "typescript": "^3.1.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "val-loader": "^1.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.10",
    "@storybook/addon-links": "^5.1.0-alpha.36"
  }
}
